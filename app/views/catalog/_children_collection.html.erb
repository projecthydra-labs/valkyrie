<h2>Members</h2>
<table class="table table-striped">
  <tbody>
    <%= Valkyrie.config.adapter.query_service.find_inverse_references_by(model: @document.resource, property: :a_member_of).each do |child| %>
      <tr>
        <td>
          <%= link_to child.title, solr_document_path(id: "id-#{child.id}") %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
